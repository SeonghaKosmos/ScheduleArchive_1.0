<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="containerPane" alignment="BOTTOM_CENTER" prefHeight="414.0" prefWidth="100.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.Controllers.Macro.Infographics.Barcomponent.BarComponentController">
   <children>
      <Pane fx:id="infoPopupContainerPane" />
      <VBox alignment="TOP_CENTER">
         <Label fx:id="durationLabel" textFill="#e261eb">
            <font>
               <Font name="Chalkboard SE Light" size="15.0" />
            </font></Label>
         <Rectangle fx:id="freqBar" arcHeight="5.0" arcWidth="5.0" fill="#5925f5ad" height="351.0" onMouseClicked="#showInfoPopupOnMouseClicked" stroke="BLACK" strokeType="INSIDE" style="-fx-stroke-width: 0;" width="80.0">
            <VBox.margin>
               <Insets left="10.0" right="10.0" />
            </VBox.margin>
         </Rectangle>
      </VBox>

      <Label fx:id="dateLabel" text="31" textFill="#bf00ffcb">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
         <font>
            <Font name="Chalkboard SE Light" size="25.0" />
         </font>
      </Label>
   </children>
</VBox>
